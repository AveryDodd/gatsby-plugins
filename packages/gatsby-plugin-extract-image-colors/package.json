{
  "name": "gatsby-plugin-extract-image-colors",
  "version": "0.0.0",
  "description": "Plugin to extract image colors using vibrantjs",
  "main": "gatsby-node.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "author": "Mike Patterson",
  "license": "ISC",
  "dependencies": {
    "color": "^3.1.1",
    "crypto": "^1.0.1",
    "node-vibrant": "^3.2.0-alpha"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "image",
    "color",
    "vibrantjs",
    "extract"
  ]
}
